<!-- triggers -->

<triggers
   muclient_version="4.94"
   world_file_version="15"
   date_saved="2016-12-22 14:33:40"
  >
  <trigger
   expand_variables="y"
   group="choose"
   match="^  本站为鼓励新玩家的加入特设大礼，详情请查看help gift$"
   regexp="y"
   sequence="100"
  >
  <send>new
@id
@name
@superpass
@superpass
@pass
@pass
@email
@gender</send>
  </trigger>
  <trigger
   group="choose"
   match="^ 福缘：\[\s+(\d+)\s+\]"
   regexp="y"
   send_to="12"
   sequence="100"
  >
  <send>my_kar=%1</send>
  </trigger>
  <trigger
   group="choose"
   match="^ 容貌：\[\s+(\d+)\s+\]"
   regexp="y"
   send_to="12"
   sequence="100"
  >
  <send>my_per=%1</send>
  </trigger>
  <trigger
   group="choose"
   match="^ 为求取真经你已经历了0分的岁月 "
   regexp="y"
   send_to="12"
   sequence="100"
  >
  <send>target_kar = tonumber(GetVariable("target_kar"))
target_per = tonumber(GetVariable("target_per"))

if my_kar &gt;= target_kar and my_per &gt;= target_per then
  Note("选人完毕")
  Note(GetVariable("version"))
  EnableTriggerGroup("choose", false)
else
  Note( "福缘：" .. my_kar .. "    容貌：" .. my_per .. "不符合要求")
  Note(GetVariable("version"))
  Execute("quit")
  Execute("n")
  DoAfterSpecial(tonumber(GetVariable("reconnect_delay")), "Connect()", 12)
end</send>
  </trigger>
  <trigger
   group="choose"
   match="^鉴于学生的主要任务是学习，本游戏不欢迎中小学生来玩。$"
   regexp="y"
   sequence="100"
  >
  <send>no</send>
  </trigger>
  <trigger
   group="choose"
   match="^来表示，一般数值越大越好，但各项的总和是固定不变的。$"
   regexp="y"
   sequence="100"
  >
  <send>0
10
1
10
2
10
3
10</send>
  </trigger>
  <trigger
   expand_variables="y"
   group="choose"
   match="^你现有五十点天赋还没有分配。$"
   regexp="y"
   sequence="100"
  >
  <send>0
@gift_str
1
@gift_con
2
@gift_int
3
@gift_spi
9
y
score</send>
  </trigger>
  <trigger
   group="choose"
   match="^请您给自己取一个英文名字：这个名字已经被别的玩家使用了．．．$"
   regexp="y"
   send_to="12"
   sequence="100"
  >
  <send>EnableTriggerGroup("choose", false)
Note("玩家ID已经被占用。机器终止。请在变量中设定一个新的ID，然后再执行start指令")
</send>
  </trigger>
</triggers>

